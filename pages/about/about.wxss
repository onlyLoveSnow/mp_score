/* pages/about/about.wxss */
.page {
  height: 100%;
  background-color: var(--weui-BG-2)
}

.img-box {
  border-radius: 150px;
}

.weui-msg__icon-area image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 8px;
}

.my-img {
  margin: 0 100px 0 100px 0;
}

.weui-msg__extra-area {
  position: static;
}

.weui-demo-dialog {
  visibility: hidden;
  opacity: 0;
  transition: opacity 0.3s;
}

.weui-demo-dialog_show {
  visibility: visible;
  opacity: 1;
}